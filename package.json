  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "./node_modules/.bin/prettier --check src/**/*.{js,jsx}",
    "test:lint": "./node_modules/.bin/eslint --cache './{**/*,*}.{js,jsx}'",
    "test:lint:fix": "./node_modules/.bin/eslint --cache './{**/*,*}.{js,jsx}' --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "import-sort-style-module": "^6.0.0"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "none",
    "jsxSingleQuote": true,
    "arrowParens": "avoid",
    "endOfLine": "auto"
  },
  "importSort": {
    ".js, .jsx": {
      "style": "module"
    }
  }
}
